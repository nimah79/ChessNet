<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.ProfileController">
	<children>
		<Label layoutX="263.0" layoutY="76.0" text="Profile">
			<font>
				<Font name="System Bold" size="21.0" />
			</font>
		</Label>
		<Button fx:id="dashboardBtn" layoutX="24.0" layoutY="97.0"
			mnemonicParsing="false" onAction="#showDashboardPage"
			prefHeight="36.0" prefWidth="193.0" text="Dashboard" />
		<Button fx:id="historyBtn" layoutX="24.0" layoutY="137.0"
			mnemonicParsing="false" onAction="#showHistoryPage" prefHeight="36.0"
			prefWidth="193.0" text="Games History">
		</Button>
		<Button fx:id="profileBtn" layoutX="24.0" layoutY="177.0"
			mnemonicParsing="false" onAction="#showProfilePage" prefHeight="36.0"
			prefWidth="193.0" text="Profile" />
		<Button fx:id="aboutBtn" layoutX="24.0" layoutY="218.0"
			mnemonicParsing="false" onAction="#showAboutPage" prefHeight="36.0"
			prefWidth="193.0" text="About" />
		<Button fx:id="logoutBtn" layoutX="24.0" layoutY="258.0"
			mnemonicParsing="false" onAction="#logout" prefHeight="36.0"
			prefWidth="193.0" text="Logout" />
		<Text fx:id="usernameLbl" layoutX="263.0" layoutY="137.0"
			strokeType="OUTSIDE" strokeWidth="0.0" text="Username: username">
			<font>
				<Font name="System Bold" size="15.0" />
			</font>
		</Text>
		<Text layoutX="265.0" layoutY="210.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="New password:" />
		<Text layoutX="263.0" layoutY="178.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="Change password">
			<font>
				<Font name="System Bold" size="15.0" />
			</font>
		</Text>
		<PasswordField fx:id="passwordField" layoutX="369.0"
			layoutY="189.0" prefHeight="31.0" prefWidth="399.0"
			promptText="New password" />
		<ButtonBar layoutX="265.0" layoutY="230.0" prefHeight="40.0"
			prefWidth="200.0" />
		<Button fx:id="changePasswordBtn" layoutX="369.0"
			layoutY="225.0" mnemonicParsing="false" onAction="#changePassword"
			prefHeight="31.0" prefWidth="399.0" text="Change password" />
	</children>
</AnchorPane>
